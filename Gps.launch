<launch>
  <!-- NavSat serial comms -->
  <group ns="gps">
    <node pkg="nmea_navsat_driver" type="nmea_serial_driver" name="nmea_serial_node" output="screen">
      <param name="port" value="/dev/ttyUSB0" />
      <param name="baud" value="9600" />
    </node>

    <!-- ENU translation --> 
    <node pkg="enu" type="from_fix" name="enu_from_fix" output="screen">
      <param name="invalid_covariance_value" value="1000000" />
      <param name="lock_altitude" value="0"/>
      <param name="scale_covariance" value="0.05"/>
      <!--remap from="enu" to="/gps"/-->
    </node>

    <node pkg="capra_gps" type="enu_rotator.py" name="EnuRotator">
    </node>
  </group>
</launch>